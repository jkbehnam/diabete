<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape>
        <solid android:color="#BE607D8B"></solid>
        <stroke android:color="#92607D8B" android:width="0.1dp" ></stroke>

    </shape>
</item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#F7607D8B"></solid>
            <stroke android:color="@color/grey_5" android:width="0.1dp" ></stroke>
        </shape>
    </item>

</selector>